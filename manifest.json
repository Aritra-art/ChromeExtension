{
  "name": "Tab Title",
  "description": "A extension which fetches the Title of the Present Tab",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}
